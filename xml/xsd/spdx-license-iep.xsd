    <xs:annotation>
      <xs:documentation>A data type for License author's preferred text to indicated that a file is covered by the license.</xs:documentation>
      <xs:appinfo>
        <Element xmlns="" name="standardLicenseHeader" xmlname="StandardLicenseHeader" comment="License author's preferred text to indicated that a file is covered by the license." rdf="http://spdx.org/rdf/terms#standardLicenseHeader" domain="License" range="String"/>
